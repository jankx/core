{
  "name": "jankx/core",
  "description": "This is the core of Jankx theme framework and is also usable on its own as embed framework",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Puleeno Nguyen",
      "email": "puleeno@gmail.com"
    }
  ],
  "require": {
    "psr/container": "^2.0",
    "jankx/admin": "*",
    "jankx/asset": "*",
    "jankx/command": "*",
    "jankx/helpers": "^1.0.0",
    "jankx/languages": "*",
    "jankx/option-adapter": "*",
    "jankx/site-layout": "*",
    "jankx/post-layout": "*",
    "jankx/template": "^1.0.0",
    "jankx/pagination": "*",
    "jankx/wordpress-specs": "*",
    "symfony/serializer": "^5.4",
    "symfony/property-access": "^5.4",
    "masterminds/html5": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "": "./src/",
      "AdvancedElementor\\Controls\\": "./src/Jankx/Integration/Elementor/Controls/",
      "Illuminate\\Contracts\\Container\\": "./src/Jankx/Interfaces/Container/",
      "Illuminate\\Container\\": "./src/Jankx/Container/"
    },
    "files": [
      "framework.php"
    ]
  },
  "require-dev": {
    "jankx/debugger": "*"
  },
  "suggest": {
    "monolog/monolog": "Allows more advanced logging of the application flow",
    "jankx/dashboard-framework": "Official option framework for Jankx theme"
  }
}
